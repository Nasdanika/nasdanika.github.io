      <sections uuid="da0bfa56-6b62-42e8-bff9-963c80b2ec40" id="1dd6c68c-0820-4991-a54e-eef91422bd7a" text="Options">
        <sections uuid="f0a0d978-5477-458e-bed1-86a56f92b261" id="2c73c559-025d-48fa-ac96-ab2d39c48dfb" text="-F, --representation-element-filter" icon="https://docs.nasdanika.org/images/filter.svg" name="opt-F">
