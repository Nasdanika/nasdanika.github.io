          <content xsi:type="org.nasdanika.exec.content:Text" uuid="5c2e4463-19cf-40bf-b293-b40f1d705d49" content="&lt;div class=&quot;markdown-body&quot;>&#xD;&#xA;&#xD;&#xA;&lt;/div>&#xD;&#xA;"/>
        </sections>
      </sections>
    </children>
